{
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "/admin/:path*",
      "has": [
        {
          "type": "host",
          "value": "admin\\..*"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/admin/:path*",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    }
  ]
}
