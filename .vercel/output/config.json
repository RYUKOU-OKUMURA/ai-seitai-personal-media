{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/blog/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/events/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/events/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/ping/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/callback/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/logout/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/me/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/youtube/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}